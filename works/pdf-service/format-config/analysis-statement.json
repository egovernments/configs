{
  "key": "analysis-statement",
  "config": {
    "pageSize": "A4",
    "pageOrientation": "portrait",
    "content": [
      {
        "style": "noc-head",
        "table": {
          "widths": [
            60,
            "*",
            230
          ],
          "height": 600,
          "body": [
            [
              {
                "image": "{{logoImage}}",
                "width": 50,
                "height": 61.25,
                "margin": [
                  10,
                  10,
                  0,
                  0
                ]
              },
              {
                "stack": [
                  {
                    "text": "{{pdf_header}} {{subdivision}}",
                    "fontSize": 18,
                    "color": "#000000",
                    "bold": true
                  },
                  {
                    "text": "{{address}}",
                    "fontSize": 10,
                    "color": "#000000"
                  },
                  {
                    "text": "{{phoneNumber}} | {{email}}",
                    "fontSize": 10,
                    "color": "#000000"
                  }
                ],
                "alignment": "left",
                "margin": [
                  10,
                  10,
                  0,
                  0
                ]
              },
              {
                "stack": [
                  {
                    "text": "{{rateAnalysisType}} {{ANALYSIS}}",
                    "style": {
                      "bold": true,
                      "fontSize": 18,
                      "color": "#5f5f5f"
                    }
                  },
                  {
                    "text": "{{WRK_ESTIMATE}} : {{estimateNumber}}",
                    "style": {
                      "fontSize": 14
                    }
                  }
                ],
                "alignment": "right",
                "margin": [
                  0,
                  10,
                  15,
                  0
                ]
              }
            ]
          ]
        },
        "layout": "noBorders",
        "fillColor": "#f4f4f4"
      },
      {
        "margin": [
          0,
          20,
          0,
          20
        ],
        "table": {
          "widths": [
            "30%",
            "70%"
          ],
          "body": [
            [
              {
                "text": "{{ProjectID}}",
                "fontSize": 15,
                "bold": true
              },
              {
                "text": "{{projectId}}",
                "fontSize": 15
              }
            ],
            [
              {
                "text": "{{ProjectName}}",
                "fontSize": 15,
                "bold": true
              },
              {
                "text": "{{projectName}}",
                "fontSize": 15
              }
            ],
            [
              {
                "text": "{{ProjectLocation}}",
                "fontSize": 15,
                "bold": true
              },
              {
                "text": "{{locality}} , {{ward}} , {{city}}",
                "fontSize": 15
              }
            ],
            [
              {
                "text": "{{ProjectDescription}}",
                "fontSize": 15,
                "bold": true
              },
              {
                "text": "{{projectDescription}}",
                "fontSize": 15
              }
            ]
          ]
        },
        "layout": "noBorders"
      },
      {
        "text": "{{rateAnalysisType}} {{WRK_DETAILS}}",
        "style": "header"
      },
      {
        "margin": [
          -20,
          0,
          20,
          0
        ],
        "table": {
          "headerRows": 1,
          "widths": [
            "6%",
            "48%",
            "6%",
            "10%",
            "15%",
            "15%"
          ],
          "body": [
            [
              {
                "style": "table-colums-header",
                "text": "{{SNo}}"
              },
              {
                "style": "table-colums-header",
                "text": "{{EstimateDescription}}"
              },
              {
                "style": "table-colums-header",
                "text": "{{EstimateUOM}}"
              },
              {
                "style": "table-colums-header",
                "text": "{{EstimateRate}}",
                "alignment": "right"
              },
              {
                "style": "table-colums-header",
                "text": "{{EstimateQuantity}}",
                "alignment": "right"
              },
              {
                "style": "table-colums-header",
                "text": "{{EstimateAmount}}",
                "alignment": "right"
              }
            ],
            "{{#tableData}}[{\"text\":\"{{Sno}}\",\"alignment\":\"left\",\"fontSize\":9},{\"text\":\"{{description}}\",\"alignment\":\"left\",\"fontSize\":9},{\"text\":\"{{unit}}\",\"alignment\":\"left\",\"fontSize\":9},{\"text\":\"{{rate}}\",\"alignment\":\"right\",\"fontSize\":9},{\"text\":\"{{quantity}}\",\"alignment\":\"right\",\"fontSize\":9},{\"text\":\"{{amount}}\",\"alignment\":\"right\",\"fontSize\":9}]{{/tableData}}",

            [
              {
                "text": "{{TotalEstimateAmount}}",
                "colSpan": 5,
                "style": "table_last_row",
                "alignment": "right",
                "margin": [
                  350,
                  0,
                  0,
                  0
                ]
              },
              {},
              {},
              {},
              {},
              {
                "text": "{{totalSum}}",
                "colSpan": 1,
                "style": "table_last_row",
                "alignment": "right",
                "margin": [
                  0,
                  0,
                  0,
                  0
                ]
              }
            ]
          ]
        },
        "layout": {
          "hLineColor": "#D6D5D4",
          "vLineColor": "#D6D5D4"
        }
      }
    ],
    "footer": "function(currentPage, pageCount) { return { text: 'Page ' + currentPage.toString() + ' of ' + pageCount.toString() + ' ', alignment: 'right' }; }",
    "styles": {
      "header": {
        "fontSize": 16,
        "bold": true,
        "margin": [
          -20,
          40,
          10,
          10
        ]
      },
      "noc-head": {
        "margin": [
          -40,
          -40,
          -40,
          1
        ]
      },
      "receipt-logo-header": {
        "color": "#000000",
        "fontSize": 14,
        "bold": true,
        "letterSpacing": 0.74,
        "margin": [
          0,
          0,
          0,
          5
        ]
      },
      "table-colums-header": {
        "fillColor": "#eeeeee",
        "bold": true,
        "fontSize": 9
      },
      "sl-no-table-row": {
        "margin": [
          5,
          0,
          0,
          0
        ],
        "fontSize": 9
      },
      "table_last_row": {
        "bold": true,
        "fillColor": "#eeeeee"
      }
    }
  }
}
